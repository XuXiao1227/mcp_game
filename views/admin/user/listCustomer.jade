.container-fluid(style="padding-left:0px")
    form.form-inline
        label.control-label(for=id + '_username', style="font-size:16px;") 用户名&nbsp;&nbsp;
        input.form-control(id=id + '_username', type='text', placeholder="用户名", value=cond.username)
.container-fluid(style="padding-left:0px")
    .row(style="padding-top:6px;")
        .col-sm-12
            button.btn.btn-default(id=id + '_search', type="button", data-loading-text="处理中..", autocomplete="off") 搜索
.container-fluid(style="padding-left:0px;padding-top:6px;")
    table.table.table-striped.table-bordered.table-hover
        thead
            tr
                td 用户名
                td 注册时间
                td 版本
                td 渠道
                td 登录方式
                td ID
                td 状态
                td 帐户
        tbody
            - each val in rst
                tr
                    td=val.username
                    td=val.reg_time
                    td=val.client_type.des
                    td=val.channel.des
                    td=val.reg_type.des
                    td=val.id
                    td=val.status.des
                    td
                        button.btn.btn-default(type="button", flag=id + "_detail", tId=val.id) 详情
.container-fluid(id=id + '_pageBar', style="padding-left:0px;")
script(type='text/javascript', src='./js/' + jadePath + '.js')
script(type='text/javascript')
    |CurSite.IMPL['#{id}'] = new CurSite.COMP.AdminUserListCustomer({id:'#{id}', parent:'#{parent}', skip:'#{skip}', limit:'#{limit}', count:'#{count}'});